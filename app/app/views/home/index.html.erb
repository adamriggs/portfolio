


<div id="image_box">

<% $projects.each do |project| %>
	
	<div class="col">
	
		<div class="project-tile">
			<img src="<%= project.thumb.to_s %>"><br>
			Agency: <%= project.agency.to_s %><br>
			Client: <%= project.client.to_s %><br>
			Description: <%= project.description.to_s %><br>
			<a><%= project.link.to_s %></a>
		</div>
	
	</div>
	
<% end %>

</div>